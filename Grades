#include <stdio.h>
int main(void){
int num;
printf("Enter your mark ");
scanf("%d",&num);
printf(" You entered %d", num); // printing outputs

	if(num >= 85){
	printf(" Grade A"); // printing outputs
		}
	else if ( num >=70){ // Note the space between else & if
		printf(" Grade B");
		}
	else if ( num >=55){
		printf(" Grade C ");
		}
	else if ( num >= 40){
		printf(" Grade D");
		}
  else if ( num < 40){
    printf(" Grade F");
    }
return 0;
}
